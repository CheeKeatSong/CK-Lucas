<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.point2points.kdusurveysystem">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/kdu_logo_invert"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity android:name=".LaunchingScreen"
                  android:configChanges="orientation"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Login"
                  android:configChanges="orientation"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".StudentMainActivity">
        </activity>
        <activity android:name=".AdminMainActivity">
        </activity>
        <activity android:name=".RecylcerView.RecyclerViewExample">
        </activity>

    </application>

</manifest>